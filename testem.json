{
  "framework": "mocha",
  "launch_in_dev": ["SAFE", "Consumption", "misc"],
  "launch_in_ci": ["SAFE", "Consumption", "misc"],
  "launchers": {
    "SAFE": {
      "command": "npm test -- --opts test/1SAFE/mocha.opts",
      "protocol": "tap"
    },
    "Consumption": {
      "command": "npm test -- --opts test/2consumption/mocha.opts",
      "protocol": "tap"
    },
    "misc": {
      "command": "npm test -- --opts test/mocha-misc.opts",
      "protocol": "tap"
    }
  },
  "src_files": [
    "test/**/*.js",
    "test/**/*.coffee",
    "lib/**/*.js",
    "na-loader/**/*.js",
    "na-loader/**/*.coffee"
  ]
}
