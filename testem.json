{
  "framework": "mocha",
  "launch_in_dev": ["Node"],
  "launch_in_ci": ["Node"],
  "launchers": {
    "Node": {
      "command": "mocha -R respec-given/mocha/tap-reporter",
      "protocol": "tap"
    }
  },
  "src_files": [
    "test/**/*.js",
    "test/**/*.coffee",
    "lib/**/*.js",
    "na-loader/**/*.js",
    "na-loader/**/*.coffee"
  ]
}
